{
  "name": "@mollerse/midi-control",
  "version": "2.4.0",
  "description": "Functionality for binding a tweakpane pane to a MIDI Device",
  "author": "Stian MÃ¸llersen <stian.moellersen@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mollerse/midi-control.git"
  },
  "bugs": {
    "url": "https://github.com/mollerse/midi-control/issues"
  },
  "homepage": "https://github.com/mollerse/midi-control#readme",
  "keywords": [
    "tweakpane",
