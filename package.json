  "scripts": {
    "test:web": "vite ./test/web",
    "test:node": "node ./test/node/test.js",
    "lint": "eslint .",
    "format": "prettier . -w",
    "typecheck": "tsc -p ./jsconfig.json --noEmit"
  },
  "dependencies": {
    "tweakpane": "^4.0.4"
  },
  "optionalDependencies": {
    "midi": "^2.0.0"
